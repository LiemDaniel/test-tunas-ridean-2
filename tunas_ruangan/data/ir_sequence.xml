<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Room Booking -->
        <record id="seq_tunas_room_book" model="ir.sequence">
            <field name="name">Room Booking Sequence</field>
            <field name="code">tunas.room.book.sequence</field>
            <field name="prefix">%(year)s/%(month)s/%(day)s/</field>
